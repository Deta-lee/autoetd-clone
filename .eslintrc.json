{
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "airbnb-typescript",
        "plugin:prettier/recommended",
        "prettier/react",
        "prettier/@typescript-eslint"
    ],
    "rules": {
        "react/prop-types": "off",
        "import/prefer-default-export": "off"
    }
}
